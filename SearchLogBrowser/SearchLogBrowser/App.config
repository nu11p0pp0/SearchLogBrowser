<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework" requirePermission="false" />
  </configSections>
  <connectionStrings>
    <add name="Default" connectionString="host=localhost;port=5432;database=slbrowser;user id=postgres;password=postgres;" providerName="Npgsql" />
  </connectionStrings>
  <entityFramework>
    <defaultConnectionFactory type="Npgsql.NpgsqlConnectionFactory, EntityFramework6.Npgsql" />
    <providers>
      <provider invariantName="Npgsql" type="Npgsql.NpgsqlServices, EntityFramework6.Npgsql" />
    </providers>
  </entityFramework>
  <system.data>
    <DbProviderFactories>
      <add name="Npgsql Data Provider" invariant="Npgsql" support="FF" description=".Net Framework Data Provider for Postgresql" type="Npgsql.NpgsqlFactory, Npgsql" />
    </DbProviderFactories>
  </system.data>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <appSettings>
    <add key="searchEngineList" value="[            {'Key':'Google', 'Value':'https://www.google.com/search?q=', 'Title':'ggrks'}            , {'Key':'Yahoo!!', 'Value':'https://search.yahoo.co.jp/search?p=', 'Title':'yhrks'}            , {'Key':'Bing', 'Value':'https://www.bing.com/search?q=', 'Title':'bngrks'}            , {'Key':'DuckDuckGo', 'Value':'https://duckduckgo.com/?q=', 'Title':'ddgrks'}            ]" />
    <add key="id" value="nullpo-no-kei" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Npgsql" publicKeyToken="5d8b90d52f46fda7" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.0.0" newVersion="4.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>